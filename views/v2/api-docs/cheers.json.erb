{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"http://localhost:4567/v2",
  "resourcePath":"/cheers",
  "apis":[
    {
      "path":"/cheers/available",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Available cheers",
          "notes":"Cheers refilled on Monday.",
          "responseClass":"List[CheerType]",
          "nickname":"available",
          "parameters":[],
          "errorResponses":<%= responses(400) %>
        }
      ]
    },
    {
      "path":"/cheers/all",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"List of all cheer types",
          "responseClass":"List[CheerType]",
          "nickname":"index",
          "parameters":[],
          "errorResponses":<%= responses(400) %>
        }
      ]
    }
  ],
  "models":<%= models('CheerType') %>
}
