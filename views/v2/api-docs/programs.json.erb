{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"<%= Const::BASE_URL %>/v2",
  "resourcePath":"/programs",
  "apis":[
    {
      "path":"/programs",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"List of available programs",
          "responseClass":"List[Program]",
          "nickname":"index",
          "parameters":[
            <%= param_page %>,
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400) %>
        }
      ]
    },
    {
      "path":"/programs/{id}",
      "description":"Operations about programs",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Find program by ID",
          "responseClass":"Program",
          "nickname":"show",
          "parameters":[
            {
              "name":"id",
              "description":"The id of the program.",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(404) %>
        }
      ]
    }
  ],
  "models":<%= models(params[:api_version], 'Program') %>
}
