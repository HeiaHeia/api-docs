{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"<%= Const::BASE_URL %>/v2",
  "resourcePath":"/pymk",
  "apis":[
    {
      "path":"/pymk",
      "description":"List of PYMK",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"List of PYMK",
          "responseClass":"List[CompactUser]",
          "nickname":"index",
          "parameters":[
            <%= param_page %>,
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    },
    {
      "path":"/pymk/search",
      "description":"Search for PYMK",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Search PYMK",
          "responseClass":"List[CompactUser]",
          "nickname":"search",
          "parameters":[
            {
              "name":"q",
              "paramType":"query",
              "required":true,
              "dataType":"string"
            },
            <%= param_page %>,
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    }
  ],
  "models":<%= models(params[:api_version], 'CompactUser') %>
}
