{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"<%= Const::BASE_URL %>/v2",
  "resourcePath":"/feeds",
  "apis":[
    {
      "path":"/feeds",
      "description":"Operations about feeds",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Finds feeds",
          "responseClass":"List[Feed]",
          "notes": "<%= note('feed') %>",
          "nickname":"index",
          "parameters":[
            {
              "name":"since",
              "description":"The ID of the feed entry that youâ€™ve seen.",
              "paramType":"query",
              "dataType":"long"
            },
            {
              "name":"direction",
              "description":"Direction",
              "paramType":"query",
              "dataType":"string",
              "defaultValue":"desc",
              "allowableValues":{
                "valueType":"LIST",
                "values":[
                  "desc",
                  "asc"
                ]
              }
            },
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400) %>
        }
      ]
    },
    {
      "path":"/feeds/{id}",
      "description":"Operations about feeds",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Find feed by ID",
          "responseClass":"Feed",
          "nickname":"show",
          "notes": "<%= note('feed') %>",
          "parameters":[
            {
              "name":"id",
              "description":"The id of the feed.",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(404) %>
        }
      ]
    }
  ],
  "models":<%= models(params[:api_version], 'Feed') %>
}
