{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"<%= Const::BASE_URL %>/v2",
  "resourcePath":"/weekly_summaries",
  "apis":[
    {
      "path":"/weekly_summaries",
      "description":"Operations about weekly summaries",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Finds weekly summaries",
          "responseClass":"List[WeeklySummary]",
          "nickname":"index",
          "parameters":[
            <%= param_page %>,
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400) %>
        }
      ]
    },
    {
      "path":"/weekly_summaries/{id}",
      "description":"Operations about weekly summaries",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Find summary by ID",
          "responseClass":"WeeklySummary",
          "nickname":"show",
          "parameters":[
            {
              "name":"id",
              "description":"The id of the weekly summary.",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(404) %>
        }
      ]
    }
  ],
  "models":<%= models(params[:api_version], 'WeeklySummary') %>
}
