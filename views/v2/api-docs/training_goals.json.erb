{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"<%= Const::BASE_URL %>/v2",
  "resourcePath":"/training_goals",
  "apis":[
    {
      "path":"/training_goals",
      "description":"Operations about training goals",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Finds training goals",
          "notes":"Finds training goals",
          "responseClass":"List[TrainingGoal]",
          "nickname":"index",
          "parameters":[
            <%= param_year %>,
            <%= param_page %>,
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400) %>
        },
        <%= erb :'shared/training_goals/_create.json' %>
      ]
    },
    {
      "path":"/training_goals/{id}",
      "description":"Operations about training goals",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Get a training goal by ID",
          "responseClass":"TrainingGoal",
          "nickname":"show",
          "parameters":[
            {
              "name":"id",
              "description":"The id of the training goal",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(404) %>
        },
        {
          "httpMethod":"PUT",
          "summary":"Update an existing training goal",
          "responseClass":"TrainingGoal",
          "nickname":"update",
          "parameters":[
            {
              "name":"id",
              "description":"ID of training goal that needs to be updated",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            },
            <%= param_notes %>,
            {
              "name":"private",
              "paramType":"form",
              "dataType":"boolean"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        },
        {
          "httpMethod":"DELETE",
          "summary":"Delete training goal",
          "responseClass":"void",
          "nickname":"destroy",
          "parameters":[
            {
              "name":"id",
              "description":"ID of training goal that needs to be deleted",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    },
    <%= erb :'shared/_years.json', { :locals => { :resource => '/training_goals/years' } } %>,
    <%= erb :'shared/_comment.json', { :locals => { :resource => 'training_goals/{training_goal_id}' } } %>,
    <%= erb :'shared/_cheer.json', { :locals => { :resource => 'training_goals/{training_goal_id}' } } %>
  ],
  "models":<%= models('TrainingGoal', 'Cheer', 'Comment') %>
}
