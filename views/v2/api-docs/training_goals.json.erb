{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"http://localhost:4567/v2",
  "resourcePath":"/training_goals",
  "apis":[
    {
      "path":"/training_goals",
      "description":"Operations about training goals",
      "operations":[
        {
          "httpMethod":"POST",
          "summary":"Add a new training goal",
          "responseClass":"TrainingGoal",
          "nickname":"create",
          "parameters":[
            {
              "name":"notes",
              "paramType":"form",
              "dataType":"text",
              "description":"Informal notes for the training goal."
            },
            {
              "name":"private",
              "paramType":"form",
              "dataType":"boolean"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    },
    {
      "path":"/training_goals/{id}",
      "description":"Operations about training goals",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Get a training goal by ID",
          "responseClass":"TrainingGoal",
          "nickname":"show",
          "parameters":[
            {
              "name":"id",
              "description":"The id of the training goal",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(404) %>
        },
        {
          "httpMethod":"PUT",
          "summary":"Update an existing training goal",
          "responseClass":"TrainingGoal",
          "nickname":"update",
          "parameters":[
            {
              "name":"id",
              "description":"ID of training goal that needs to be updated",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            },
            {
              "name":"notes",
              "paramType":"form",
              "dataType":"text",
              "description":"Informal notes for the training goal."
            },
            {
              "name":"private",
              "paramType":"form",
              "dataType":"boolean"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        },
        {
          "httpMethod":"DELETE",
          "summary":"Delete training goal",
          "responseClass":"void",
          "nickname":"destroy",
          "parameters":[
            {
              "name":"id",
              "description":"ID of training goal that needs to be deleted",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    },
    <%= erb :'shared/_comment.json', { :locals => { :resource => 'training_goals', :param_id => 'training_goal_id' } } %>,
    <%= erb :'shared/_cheer.json', { :locals => { :resource => 'training_goals', :param_id => 'training_goal_id' } } %>
  ],
  "models":<%= models('TrainingGoal', 'Cheer', 'Comment') %>
}
