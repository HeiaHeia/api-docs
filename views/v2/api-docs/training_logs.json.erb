        },
        "duration_m": {
          "type": "int"
        },
        "duration_s": {
          "type": "int"
        },
        "avg_hr": {
          "type": "int"
        },
        "max_hr": {
          "type": "int"
        },
        "sport_params":{
          "items":{
            "$ref":"SportParam"
          },
          "type":"Array"
        },
        "favourite":{
          "type":"boolean"
        },
        "private":{
          "type":"boolean"
        },
        "cheers_count": {
          "type": "int"
        },
        "comments_count": {
          "type": "int"
        },
        "distance": {
          "type": "int"
        },
        "steps": {
          "type": "int"
        },
        "exclude_stats": {
          "type":"boolean"
        },
        "cheerable":{
          "type":"boolean"
        },
        "commentable":{
          "type":"boolean"
        },
        "place":{
          "type":"Place"
        },
        "updated_at":{
          "type":"DateTime",
          "required": true
        },
        "created_at":{
          "type":"DateTime",
          "required": true
        },
        "gpx_url":{
          "type":"string"
        }
      }
    }
  }
}
