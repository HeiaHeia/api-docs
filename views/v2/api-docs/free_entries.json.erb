{
  "apiVersion":"2",
  "swaggerVersion":"1.2",
  "basePath":"http://localhost:4567/v2",
  "resourcePath":"/free_entries",
  "apis":[
    {
      "path":"/free_entries",
      "description":"Operations about free entries",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Finds free entries",
          "responseClass":"List[FreeEntry]",
          "nickname":"index",
          "parameters":[
            <%= param_year %>,
            <%= param_page %>,
            <%= param_per_page %>
          ],
          "errorResponses":<%= responses(400) %>
        },
        {
          "httpMethod":"POST",
          "summary":"Add a new free entry",
          "responseClass":"FreeEntry",
          "nickname":"create",
          "parameters":[
            <%= param_required_date %>,
            <%= param_notes %>,
            {
              "name":"private",
              "paramType":"form",
              "dataType":"boolean"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    },
    {
      "path":"/free_entries/{id}",
      "description":"Operations about free entries",
      "operations":[
        {
          "httpMethod":"GET",
          "summary":"Find free entry by ID",
          "responseClass":"FreeEntry",
          "nickname":"show",
          "parameters":[
            {
              "name":"id",
              "description":"The id of the free entry.",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(404) %>
        },
        {
          "httpMethod":"PUT",
          "summary":"Update an existing free entry",
          "responseClass":"FreeEntry",
          "nickname":"update",
          "parameters":[
            {
              "name":"id",
              "description":"ID of free entry that needs to be updated",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            },
            <%= param_date %>,
            <%= param_notes %>,
            {
              "name":"private",
              "paramType":"form",
              "dataType":"boolean"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        },
        {
          "httpMethod":"DELETE",
          "summary":"Delete free entry",
          "responseClass":"void",
          "nickname":"destroy",
          "parameters":[
            {
              "name":"id",
              "description":"ID of free entry that needs to be deleted",
              "paramType":"path",
              "required":true,
              "dataType":"long"
            }
          ],
          "errorResponses":<%= responses(400, 404) %>
        }
      ]
    },
    <%= erb :'shared/_comment.json', { :locals => { :resource => 'free_entries/{free_entry_id}' } } %>,
    <%= erb :'shared/_cheer.json', { :locals => { :resource => 'free_entries/{free_entry_id}' } } %>
  ],
  "models":<%= models('FreeEntry', 'Cheer', 'Comment') %>
}
